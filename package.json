{
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.145.0",
    "@marp-team/marp-cli": "^2.1.2",
    "animate.css": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^3.3.0",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.2.3",
    "prettier": "^2.7.1",
    "tsup": "^6.6.3",
    "wait-on": "^7.0.1"
  },
  "packageManager": "npm@8.11.0",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:express": "tsup express/index.js",
    "build:nuxt": "nuxi build",
    "dev": "dotenv -e .env -- run-p dev:*",
    "dev:express": "tsup express/index.js --watch express/ --onSuccess \"node dist/index.js\"",
    "dev:nuxt": "run-s dev:nuxt:{wait,dev}",
    "dev:nuxt:dev": "nuxi dev",
    "dev:nuxt:wait": "wait-on http://localhost:4000/api/",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "slide": "marp -s docs",
    "start": "run-p start:*",
    "start:express": "node dist/index.js",
    "start:nuxt": "node .output/server/index.mjs",
    "update": "npm-check-updates --deep --upgrade && npm dedupe"
  }
}
